{
  "name": "bahadorbzd/laravel-twilio-notification-channel",
  "description": "A Laravel notification channel for sending SMS via Twilio.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Bahador",
      "email": "your-email@example.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "twilio/sdk": "^7.0",
    "illuminate/notifications": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Bahadorbzd\\TwilioNotificationChannel\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bahadorbzd\\TwilioNotificationChannel\\TwilioSmsChannelServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}